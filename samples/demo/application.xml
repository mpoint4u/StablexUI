<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="StablexUI_Android_demo" package="com.example.StablexUIAndroiddemo" version="1.0.0" company="pm" />
	
	<!-- output -->
	<app main="com.example.Main" file="StablexUIAndroiddemo" path="bin" />
	
    <window width="320" height="480" background="0x888888" fps="60"/>
    <window width="0" height="0" fullscreen="true" if="mobile"/>
	
	<!-- classpath, haxe libs -->
    <source name="../../src/" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
    <set name="html5-backend" value="openfl-html5-dom" />	
	
	<!-- assets 
<assets path="assets/img" rename="img" />
	-->
	<icon path="assets/openfl.svg" />
    <assets path="ui/android" include="*" exclude="defaults.xml|skins.xml"/>	
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>